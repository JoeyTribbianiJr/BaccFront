<Window x:Class="Bacc_front.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bacc_front"
        mc:Ignorable="d"
        Title="软件注册" Height="200" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="60"></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center">机器码：</TextBlock>
        <TextBox IsEnabled="False" Height="26" Name="txtMachineCode"
                 Grid.Column="1"></TextBox>
        <TextBlock VerticalAlignment="Center" Grid.Row="1"
                   HorizontalAlignment="Center">注册码：</TextBlock>
        <TextBox IsEnabled="True" Height="26" Name="txtRegisterCode"
                 Grid.Row="1" Grid.Column="1"></TextBox>
    </Grid>
</Window>
